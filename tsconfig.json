{
	"compilerOptions": {
	  "target": "es6",
	  "module": "esnext",
	  "moduleResolution": "node",
	  "strict": true,
	  "jsx": "preserve",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "baseUrl": ".",
	  "paths": {
		"$types": ["src/types.d.ts"]
	  }
	},
	"include": ["src/**/*.ts", "src/**/*.svelte"],
	"exclude": ["node_modules"]
  }
  
  